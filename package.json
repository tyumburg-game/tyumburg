{
  "name": "tyumburg",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:ssr": "concurrently \"npm run start:server\" \"npm run start:client:ssr\"",
    "start:client:ssr": "webpack --config ./webpack-configs/webpack.ssr.js --watch",
    "start:server": "node ./ssr/server/index.js",
    "start:dev": "npm run compile:sw & webpack serve --config ./webpack-configs/webpack.dev.js",
    "build:client:prod": "npm run compile:sw & webpack --config ./webpack-configs/webpack.prod.js",
    "build:client:ssr": "npm run compile:sw & webpack --config ./webpack-configs/webpack.ssr.js",
    "build:server": "tsc --project ./server",
    "compile:sw": "tsc sw.ts --outFile ./dist/sw.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "stylelint": "npx stylelint \"./src/**/*.css\"",
    "eslint": "eslint ./src --ext .ts,.tsx",
    "eslint:fix": "eslint ./src --fix --ext .ts,.tsx",
    "prettier:check": "prettier --check .",
    "prepare": "husky install",
    "docker:build": "docker build -t tyumburg ."
  },
  "engines": {
    "node": ">=16"
  },
